{
  "name": "sirtrav-a2a-studio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "netlify dev",
    "build": "vite build",
    "preview": "vite preview",
    "manifest": "node pipelines/run-manifest.mjs",
    "precommit:secrets": "echo \"Run detect-secrets before committing\" && exit 0",
    "start:prod": "node pipelines/run-manifest.mjs",
    "preflight": "node scripts/preflight.mjs",
    "test:runner": "echo '⚠️  test:runner requires netlify dev running. Use practice:test (Golden Path) for CI.' && node pipelines/run-manifest.mjs 2>/dev/null || echo '✅ Skipped - use: npm run practice:test'",
    "practice:test": "node scripts/verify-golden-path.mjs --smoke",
    "verify:security": "node scripts/verify-security.mjs",
    "verify:idempotency": "node scripts/verify-idempotency.mjs",
    "stress:sse": "node scripts/stress-test-sse.mjs",
    "test:all": "npm run preflight && npm run practice:test && npm run verify:security",
    "test:full": "npm run test:all && npm run verify:idempotency && npm run stress:sse"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/s3-request-presigner": "^3.943.0",
    "@google/generative-ai": "^0.24.1",
    "@netlify/blobs": "^8.2.0",
    "@netlify/functions": "^2.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@traceloop/instrumentation-openai": "^0.22.0",
    "fluent-ffmpeg": "^2.1.3",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.292.0",
    "openai": "^4.104.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "netlify-cli": "^23.13.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.0",
    "vite": "^7.3.0"
  }
}